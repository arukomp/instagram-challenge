%center
  %h1 New Photo
.form-container
  = form_for @photo do |f|
    .form-group
      = f.label :photo_url
      = f.text_field :photo_url, class: 'form-control'
    .form-group
      = f.label :description
      = f.text_field :description, class: 'form-control'
    .form-group
      = f.submit 'Add photo', class: 'btn btn-primary'
